{
  "expo": {
    "name": "NutriNexus",
    "slug": "NutriNexus",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "com.supabase",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
   "plugins": [
    [
      "@react-native-google-signin/google-signin",
      {
        "iosUrlScheme": "com.googleusercontent.apps.960982903167-krh2019m7vtkcrsao5kspkqr8qlfu9af"
      }
    ],
    [
      "expo-camera",
      {
        "cameraPermission": "Allow ${PRODUCT_NAME} to access your camera",
        "microphonePermission": "Allow ${PRODUCT_NAME} to access your microphone",
        "recordAudioAndroid": true
      }
    ]
  ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "nutrinuxus.club",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "This app uses the camera to scan QR codes and take pictures."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "nutrinuxus.club"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "66588a34-081a-4bfa-9ca8-dcd4c2259840"
      }
    },
    "owner": "nutrinuxus"
  }
}
